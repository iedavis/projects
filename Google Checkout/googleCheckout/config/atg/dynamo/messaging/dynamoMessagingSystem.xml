<dynamo-message-system>
	<patchbay>	
	
	    <message-source>
      <nucleus-name>
        /com/google/checkout/messaging/FulfillmentMessageSource
      </nucleus-name>
      <output-port>
        <port-name>
          DEFAULT
        </port-name>
      </output-port>
      <output-port>
        <port-name>
          ModifyNotificationPort
        </port-name>
        <output-destination>
          <destination-name>
            patchbay:/Fulfillment/ModifyOrderNotification
          </destination-name>
          <destination-type>
            Topic
          </destination-type>
        </output-destination>
      </output-port>
    </message-source>
		
	<message-sink>
      <nucleus-name>
       /com/google/checkout/messaging/FulfillmentMessageSink
      </nucleus-name>

      <input-port>
        <port-name>
          ModifyNotificationPort
        </port-name>

	      <input-destination>
	        <destination-name>
                  patchbay:/Fulfillment/ModifyOrderNotification
                </destination-name>
                <destination-type>
                  Topic
                </destination-type>
	      </input-destination>
      </input-port>
    </message-sink>
	</patchbay>
</dynamo-message-system>